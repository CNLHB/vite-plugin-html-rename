{
	"name": "vite-plugin-html-rename",
	"version": "1.1.2",
	"description": "A Vite plugin that automatically renames HTML files based on entry names during the build process",
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "npx unbuild",
		"prepack": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CNLHB/vite-plugin-html-rename.git"
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"load html",
		"split html",
		"inject html",
		"html",
		"html-loader",
		"loader"
	],
	"author": "aiwa <1348844909@qq.com> (https://cnlhb.github.io/blog/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CNLHB/vite-plugin-html-rename/issues"
	},
	"homepage": "https://github.com/CNLHB/vite-plugin-html-rename#readme",
	"devDependencies": {
		"@types/node": "^20.11.5",
		"typescript": "^5.3.3",
		"unbuild": "^2.0.0",
		"vite": "^5.0.12"
	},
	"packageManager": "pnpm"
}